<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColorApp">

    <EditText
        android:id="@+id/edit_text_razao_social_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/razao_social"
        android:inputType="textPersonName"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edit_text_nome_fantasia_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/nome_fantasia"
        android:inputType="textPersonName"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_razao_social_organizacao"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edit_text_cnpj_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/cnpj"
        android:inputType="number"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_nome_fantasia_organizacao"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edit_text_nome_responsavel_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/nome_responsavel"
        android:inputType="textPersonName"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_cnpj_organizacao"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edit_text_email_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/email"
        android:inputType="textEmailAddress"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_nome_responsavel_organizacao"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edit_text_telefone_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/telefone"
        android:inputType="phone"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_email_organizacao"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edit_text_senha_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/senha"
        android:inputType="textPassword"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_telefone_organizacao"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edit_text_confirmar_senha_organizacao"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/confirmar_senha"
        android:inputType="textPassword"
        android:paddingEnd="0dp"
        android:paddingStart="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/textColorBlack"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_senha_organizacao"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_cadastrar_organizacao"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounded_button"
        android:fontFamily="sans-serif"
        android:text="@string/cadastrar"
        android:textAlignment="center"
        android:textColor="@color/textColorWhite"
        android:textSize="15sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_confirmar_senha_organizacao"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>